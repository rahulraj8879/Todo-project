<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootAnchorPane" prefHeight="88.0" prefWidth="320.0" style="-fx-background-color: #424242;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="iconImageView" fitHeight="34.0" fitWidth="34.0" layoutX="5.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assets/icon_tasks.png" />
         </image>
      </ImageView>
      <Label fx:id="taskLabel" layoutX="50.0" layoutY="13.0" prefHeight="24.0" prefWidth="275.0" text="Task" textFill="#f8f3f3">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="descriptionLabel" layoutX="45.0" layoutY="37.0" prefHeight="26.0" prefWidth="250.0" text="Description" textFill="#d3d3d3" wrapText="true" />
      <Label fx:id="dateLabel" alignment="CENTER_RIGHT" layoutX="5.0" layoutY="65.0" prefHeight="18.0" prefWidth="325.0" style="-fx-background-color: #fff;" text="Date Created" textFill="#19ab40" />
      <ImageView fx:id="deleteButton" fitHeight="24.0" fitWidth="33.0" layoutX="281.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assets/icon_delete.png" />
         </image>
      </ImageView>
      <ImageView fx:id="listUpdateButton" fitHeight="24.0" fitWidth="24.0" layoutX="305.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assets/icon_update.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
